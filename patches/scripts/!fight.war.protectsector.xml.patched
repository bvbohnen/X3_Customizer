<script>
<name>!fight.war.protectsector</name>
<version>6</version>
<engineversion>52</engineversion>
<description>War Scripts: Attack player assests in sector</description>
<arguments>
<argument index="1" name="a.sector" type="Var/Sector" desc="Sector" />
<argument index="2" name="a.existingShips" type="Value" desc="Existing Ship Array" />
</arguments>
<sourceplaintext>
<![CDATA[
  skip if $a.sector -> exists
    return null
  
  if not $a.existingShips
    $aCreatedShips = array alloc: size=0
  else
    $aCreatedShips = $a.existingShips
  end
  
  $x = $a.sector -> get universe x index
  $y = $a.sector -> get universe y index
  $var = sprintf: fmt='war.%s.%s', $x, $y, null, null, null
  $checkVar = get global variable: name=$var
  if $checkVar == [TRUE]
    set global variable: name=$var value=null
  else if $checkVar
    return null
  end
  set global variable: name=$var value=2
  
  $waiting = [FALSE]
  
  while $a.sector -> get WarObject ID
    skip if get global variable: name=$var
      break
    if is a new script version available
      set global variable: name=$var value=null
@     START [THIS] -> call script !fight.war.protectsector :  Sector=$a.sector  Existing Ship Array=$aCreatedShips
      return null
    end
@   = wait randomly from 500 to 1000 ms
    
  * check for existing ships
    skip if [PLAYERSHIP] -> exists
      return null
    
    $player.sector = [PLAYERSHIP] -> get sector
    if $a.sector == $player.sector
      $inPlayerSector = [TRUE]
    else
      $inPlayerSector = [FALSE]
    end
    
    if size of array $aCreatedShips
      $count = size of array $aCreatedShips
      while $count
        dec $count = 
        $ship = $aCreatedShips[$count]
        if not $ship -> exists
          remove element from array $aCreatedShips at index $count
          continue
        end
        $ship.owner = $ship -> get owner race
        if $ship.owner == Neutral Race OR $ship.owner == Player
          remove element from array $aCreatedShips at index $count
          continue
        end
        if $inPlayerSector
          if $ship -> is script !fight.war.attackplayer on stack of task=0
            $rel = $ship -> get local variable: name='war.rel'
            $ship ->set relation against Player to $rel
            START $ship -> command COMMAND_DEFEND_SECTOR : arg1=$a.sector, arg2=null, arg3=null, arg4=null
          end
        else
          if $ship -> is script !fight.defend.sector on stack of task=0
@           START $ship -> call script !fight.war.attackplayer : 
          end
        end
      end
    end
    
  * we already have ships
    do if size of array $aCreatedShips
      continue
    
  * players in sector, we dont need to create ships
    if $inPlayerSector
      $waiting = [FALSE]
      continue
    end
    
  * allow a few seconds before forces are launched
    if not $waiting
      $waiting = [TRUE]
@     = wait randomly from 10000 to 15000 ms
      continue
    end
    
    $waiting = [FALSE]
    
  * get the ships we need to attack the sector
@   $UseShips = [THIS] -> call script !lib.war.analysesector :  Sector to Analyse=$a.sector  Include Ships=[TRUE]  Include Stations=[FALSE]
    if not size of array $UseShips
@     $UseShips = [THIS] -> call script !lib.war.analysesector :  Sector to Analyse=$a.sector  Include Ships=[FALSE]  Include Stations=[TRUE]
      skip if size of array $UseShips
        continue
    end
    
    
    
  * find the race of the ships
    $race = Terran
    $noto.argon = get notoriety from race Argon to race Player
    $noto.terran = get notoriety from race Terran to race Player
    if $noto.argon < $noto.terran
      $race = Argon
    end
    
  * create the ships need to defend
    $carrier = null
    $count = size of array $UseShips
    while $count
      dec $count = 
      $shipClass = $UseShips[$count]
      $gate = find gate: flags=[Find.Random], refobj=$a.sector, max dist=null, refpos=null
      if $gate -> exists
        $x = $gate -> get x position
        $y = $gate -> get y position
        $z = $gate -> get z position
      else
        $x = 0
        $y = 0
        $z = 0
      end
      
@     $shipType = [THIS] -> call script !lib.get.randomshiptype :  Race=$race  Ship class=$shipClass  Block Array=null  Only First Variant=[TRUE]
      if $shipType
        $newShip = create ship: type=$shipType owner=$race addto=$a.sector x=$x y=$y z=$z
        $newShip ->add default items to ship
        $newShip ->set local variable: name='war.defence' value=$a.sector
@       START $newShip -> call script !fight.war.attackplayer : 
        append $newShip to array $aCreatedShips
      end
    end
  end
  
  if size of array $aCreatedShips
    $count = size of array $aCreatedShips
    while $count
      dec $count = 
      $ship = $aCreatedShips[$count]
      if $ship -> exists
        $ship ->jump out of existence
      end
    end
  end
  return null
]]>
